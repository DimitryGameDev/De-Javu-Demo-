  dialogue:
  - npcText:
    - "\u0423\u0436\u0435 \u043F\u0440\u0438\u0448\u0435\u043B ? \u0421\u0430\u0434\u0438\u0441\u044C
      \u0437\u0430 \u0441\u0442\u043E\u043B \u0431\u044B\u0441\u0442\u0440\u0435\u0435,
      \u0443 \u043C\u0435\u043D\u044F \u0443\u0436\u0435 \u0432\u0441\u0435 \u043F\u043E\u0447\u0442\u0438
      \u0433\u043E\u0442\u043E\u0432\u043E!"
    playerAnswer:
    - text: "\u0427\u0442\u043E \u0433\u043E\u0442\u043E\u0432\u043E?"
      toNode: 1
      value: 0
      exit: 0
  - npcText:
    - "\u041B\u0430\u0437\u0430\u043D\u044C\u044F, \u043A\u0430\u043A \u0442\u044B
      \u0432\u0447\u0435\u0440\u0430 \u043F\u0440\u043E\u0441\u0438\u043B! \u0423\u0436\u0435
      \u0437\u0430\u0431\u044B\u043B \u0447\u0442\u043E \u043B\u0438?!"
    playerAnswer:
    - text: "\u0410, \u0442\u043E\u0447\u043D\u043E, \u0431\u0435\u0433\u0443!"
      toNode: 2
      value: 0
      exit: 0
  - npcText:
    - "\u041D\u0440\u0430\u0432\u0438\u0442\u0441\u044F ?"
    playerAnswer:
    - text: "\u0414\u0430, \u043E\u0447\u0435\u043D\u044C \u0432\u043A\u0443\u0441\u043D\u043E!"
      toNode: 3
      value: 0
      exit: 0
  - npcText:
    - "\u0421\u043F\u0430\u0441\u0438\u0431\u043E!"
    - "\u0422\u044B \u0442\u0430\u043A \u0441\u043E\u0441\u0440\u0435\u0434\u043E\u0442\u043E\u0447\u0435\u043D\u043D\u043E
      \u0435\u0448\u044C\u2026\u0417\u0430\u0434\u0443\u043C\u0430\u043B\u0441\u044F
      \u043E \u0447\u0435\u043C-\u0442\u043E?"
    playerAnswer:
    - text: "\u0414\u0430 \u0434\u0435\u043D\u044C \u0441\u0435\u0433\u043E\u0434\u043D\u044F
        \u0441\u0442\u0440\u0430\u043D\u043D\u044B\u0439 \u0432\u044B\u0434\u0430\u043B\u0441\u044F\u2026"
      toNode: 4
      value: 0
      exit: 0
  - npcText:
    - "\u0422\u044B \u0442\u0430\u043A \u0441\u043E\u0441\u0440\u0435\u0434\u043E\u0442\u043E\u0447\u0435\u043D\u043D\u043E
      \u0435\u0448\u044C\u2026\u0417\u0430\u0434\u0443\u043C\u0430\u043B\u0441\u044F
      \u043E \u0447\u0435\u043C-\u0442\u043E?"
    playerAnswer:
    - text: "\u0414\u0430 \u044D\u0442\u043E \u0441\u043B\u043E\u0436\u043D\u043E
        \u043A\u0430\u043A-\u0442\u043E \u0442\u043E\u043B\u043A\u043E\u0432\u043E
        \u0440\u0430\u0441\u0441\u043A\u0430\u0437\u0430\u0442\u044C\u2026\u0447\u0443\u0432\u0441\u0442\u0432\u043E
        \u0431\u044B\u043B\u043E, \u043A\u0430\u043A \u0431\u0443\u0434\u0442\u043E
        \u044F \u0432 \u043A\u0430\u043A\u043E\u043C-\u0442\u043E \u0441\u0442\u0440\u0430\u043D\u043D\u043E\u043C
        \u0441\u043D\u0435 \u043D\u0430\u0445\u043E\u0436\u0443\u0441\u044C, \u0432\u043E\u0442"
      toNode: 5
      value: 0
      exit: 0
  - npcText:
    - "\u042D\u0442\u043E\u2026 \u0437\u043D\u0430\u043A\u043E\u043C\u043E\u0435
      \u0447\u0443\u0432\u0441\u0442\u0432\u043E."
    - " \u041A\u0430\u043A \u0431\u0443\u0434\u0442\u043E \u0432\u0441\u0451 \u0432\u043E\u043A\u0440\u0443\u0433\u2026
      \u043F\u0440\u0430\u0432\u0438\u043B\u044C\u043D\u043E, \u043D\u043E \u043D\u0435
      \u0441\u043E\u0432\u0441\u0435\u043C. \u041A\u0430\u043A \u0431\u0443\u0434\u0442\u043E
      \u0442\u044B \u0434\u043E\u043B\u0436\u0435\u043D \u0437\u043D\u0430\u0442\u044C,
      \u0447\u0442\u043E \u043D\u0435 \u0442\u0430\u043A, \u043D\u043E \u043D\u0435
      \u043C\u043E\u0436\u0435\u0448\u044C \u043F\u043E\u043D\u044F\u0442\u044C,
      \u0447\u0442\u043E \u0438\u043C\u0435\u043D\u043D\u043E."
    playerAnswer:
    - text: "\u0422\u0435\u0431\u044F \u0442\u0430\u043A\u043E\u0435 \u043D\u0435
        \u043F\u0443\u0433\u0430\u0435\u0442?"
      toNode: 6
      value: 0
      exit: 0
  - npcText:
    - "\u0410 \u0442\u0435\u0431\u044F? \u041D\u0435 \u043B\u043E\u0432\u0438\u0448\u044C
      \u0441\u0435\u0431\u044F \u043D\u0430 \u043C\u044B\u0441\u043B\u0438, \u0447\u0442\u043E
      \u043D\u0435 \u0443\u0432\u0435\u0440\u0435\u043D, \u043F\u0440\u043E\u0441\u043D\u0443\u043B\u0441\u044F
      \u043B\u0438?"
    - "\u0412\u0434\u0440\u0443\u0433, \u044D\u0442\u043E \u0432\u0441\u0435 \u0441\u043E\u043D
      \u0438 \u044F\u2026\u0435\u0433\u043E \u0447\u0430\u0441\u0442\u044C\u2026"
    playerAnswer:
    - text: "\u0410 \u043A\u0430\u043A \u0431\u044B \u0442\u044B \u043F\u0440\u043E\u0432\u0435\u0440\u0438\u043B\u0430
        \u044D\u0442\u043E ?"
      toNode: 7
      value: 0
      exit: 0
  - npcText:
    - "\u041E\u0431\u044B\u0447\u043D\u043E \u0433\u043E\u0432\u043E\u0440\u044F\u0442,
      \u0447\u0442\u043E \u043D\u0443\u0436\u043D\u043E \u0441\u0435\u0431\u044F
      \u0443\u0449\u0438\u043F\u043D\u0443\u0442\u044C (\u0443\u0441\u043C\u0435\u0445\u0430\u044F\u0441\u044C),
      \u043D\u043E \u043C\u043D\u0435 \u0432\u0441\u0435 \u0436\u0435 \u043A\u0430\u0436\u0435\u0442\u0441\u044F,
      \u0447\u0442\u043E \u044D\u0442\u043E \u0441\u043B\u0438\u0448\u043A\u043E\u043C
      \u043F\u0440\u043E\u0441\u0442\u043E"
    - "\u041C\u043C\u043C\u2026\u043D\u0430\u0432\u0435\u0440\u043D\u043E\u0435,
      \u044F \u0431\u044B \u043F\u043E\u043F\u0440\u043E\u0431\u043E\u0432\u0430\u043B\u0430
      \u043D\u0430\u0439\u0442\u0438 \u0437\u0434\u0435\u0441\u044C \u0447\u0442\u043E-\u0442\u043E,
      \u0447\u0435\u0433\u043E \u0437\u0434\u0435\u0441\u044C \u0431\u044B\u0442\u044C
      \u043D\u0435 \u0434\u043E\u043B\u0436\u043D\u043E, \u0443 \u0447\u0435\u0433\u043E
      \u043D\u0435\u0442 \u043E\u0431\u044A\u044F\u0441\u043D\u0435\u043D\u0438\u044F.
      (\u0417\u0430\u0434\u0443\u043C\u0447\u0438\u0432\u043E \u043E\u0441\u043C\u0430\u0442\u0440\u0438\u0432\u0430\u044F
      \u043A\u0443\u0445\u043D\u044E)"
    playerAnswer:
    - text: 
      toNode: 0
      value: 0
      exit: 1
